    '/baas/business/{documentNumber}/accounts':
        post:
            tags:
                - 'Abertura de conta jurídica (MEI, EI ou EIRELI).'
            summary: 'Crie uma conta de pagamento.'
            description: 'IMPORTANTE! É necessário ter o registro do titular (pessoa jurídica) previamente aprovado.'
            operationId: CreateBusinessAccountAsync
            parameters:
                -
                    name: documentNumber
                    in: path
                    description: 'Número do documento CNPJ da empresa. Informe somente os números.'
                    required: true
                    schema: {minLength: 1, pattern: '^[0-9]*$', type: string, description: 'Número do documento CNPJ da empresa. Informe somente os números.'}
